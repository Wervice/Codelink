{
  "main": "index.html",
  "name": "codelink",
  "window": {
    "frame": true,
    "toolbar": false,
    "icon": "images/codelink.png",
    "transparent": false
  },
  "host_permissions": [
    "https://*",
    "http://*"
  ],
  "version": "1.9",
  "scripts": {
    "dev": "nwp -r",
    "build": "nwp"
  },
  "author": "",
  "license": "MIT",
  "chromiumArgs": "--enable-transparent-visuals",
  "nwjs-packager": {
    "nwVersion": "0.84.0",
    "appFriendlyName": "tin",
    "appMacIcon": "images/tinlogo.icns",
    "appWinIcon": "images/tinlogo.ico",
    "files": [
      "images/**",
      "tin/**",
      "assets/**"
    ],
    "builds": {
      "linux": {
        "tar.gz": true
      },
      "osx": {
        "zip": true
      },
      "win": {
        "zip": false
      }
    },
    "appPackageName": "tin",
    "appVersion": "1.9",
    "appDescription": "Tiled browser",
    "appCopyright": "Wervice (Constantin Volke)"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "fs-extra": "^11.2.0",
    "mime-types": "^2.1.35",
    "node-gyp": "^10.0.1",
    "node-os-utils": "^1.3.7",
    "platform": "^1.3.6",
    "xterm": "^5.3.0"
  },
  "devDependencies": {
    "nwjs-packager": "^1.5.1"
  }
}
