{
  "main": "index.html",
  "name": "codelink",
  "window": {
    "frame": true,
    "toolbar": false,
    "icon": "images/icon.png",
    "transparent": false
  },
  "version": "1.4.1",
  "scripts": {
    "dev": "nwp -r",
    "build": "nwp"
  },
  "author": "",
  "license": "MIT",
  "chromiumArgs": "--enable-transparent-visuals",
  "nwjs-packager": {
    "nwVersion": "0.80.0",
    "appFriendlyName": "Codelink",
    "appMacIcon": "images/icon.icns",
    "appWinIcon": "images/icon.ico",
    "files": [
      "images/**",
      "./animations.css",
      "./browser.css",
      "./main.css",
      "./modal.css",
      "./browser.html",
      "./help.html",
      "./index.html",
      "./animations.js",
      "./browser.js",
      "./main.js",
      "./modal.js",
      "./favs.js",
      "./fsextra_license.txt",
      "./nwjs_license.txt",
      "./LICENSE",
      "./filetree/*",
      "./mhl_license.txt",
      "./marked_license.txt",
      "./notes_font.txt",
      "./toggle_view.txt"
    ],
    "builds": {
      "linux": {
        "tar.gz": true
      },
      "osx": {
        "zip": true
      },
      "win": {
        "zip": false
      }
    },
    "appPackageName": "Codelink",
    "appVersion": "1.5.1",
    "appDescription": "Codelink",
    "appCopyright": "Wervice (Constantin Volke)"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "jszip": "^3.10.1",
    "marked": "^9.1.5",
    "tar": "^6.2.0"
  },
  "devDependencies": {
    "nwjs-packager": "^1.5.1"
  }
}
